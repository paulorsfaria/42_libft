#include <stdio.h>
#include <string.h>

void *ft_memcpy(void *dest, const void *src, size_t n)
{
	unsigned char	*desteny;
	const unsigned char	*source;

	desteny = (unsigned char *) dest;
	source = (const unsigned char *) src;
	while(*source != '\0' && n > 0)
	{
		*desteny = *source;
		desteny++;
		source++;
		n--;
	}
	*desteny = '\0';
	return(dest);
